{
    "main": "bin/yaml-lint",
    "files": [
        "LICENSE",
        "src/yaml-lint.php",
        "src/J13k/YamlLint/UsageException.php",
        "vendor/autoload.php",
        "vendor/composer/installed.json"
    ],
    "finder": [
